</body>
    
    <div id="button4">
        <button type="button">
            <% if logged_in? %>
                <%= link_to 'View Profile', current_user %>
            <% else %>
                <%= link_to 'Log In', login_path %>
            <% end %>
        </button>
    </div>
    
    <% if logged_in? %>
        <div id="button4">
            <button type="button">
                <%= link_to 'Log out', logout_path, method: :delete %>
